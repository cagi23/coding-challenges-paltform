{
  "name": "coding-challenges",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=10000 ./node_modules/@angular/cli/bin/ng",
    "start": "npm run ng-high-memory -- serve",
    "build": "ng build",
    "build:dev": "ng build --configuration=dev",
    "build:prod": "ng build --configuration=prod",
    "test": "npm run ng-high-memory -- test --watch=true",
    "test:noWatch": "npm run ng-high-memory -- test --watch=false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "clean": "prettier --write ./**/*.{js,ts,scss,css,html,json,md} && npm run lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/localize": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@ng-select/ng-select": "^13.9.1",
    "@ngx-formly/bootstrap": "^6.3.12",
    "@ngx-formly/core": "^6.3.12",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "bootstrap": "^5.3.3",
    "bootstrap-italia": "^2.13.4",
    "bootstrap-select": "^1.14.0-beta2",
    "codelyzer": "^6.0.2",
    "codice-fiscale-js": "^2.2.4",
    "esm": "^3.2.25",
    "jquery": "^3.7.1",
    "jspdf": "^2.5.1",
    "moment": "^2.30.1",
    "ngx-mask": "^17.1.7",
    "popper.js": "^1.16.1",
    "rxjs": "~7.8.0",
    "rxjs-compat": "^6.6.7",
    "tslib": "^2.3.0",
    "tslint": "^6.1.3",
    "xng-breadcrumb": "^12.0.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "^18.2.12",
    "@angular-eslint/eslint-plugin": "^18.2.12",
    "@angular-eslint/eslint-plugin-template": "^18.2.12",
    "@angular-eslint/schematics": "^18.2.12",
    "@angular-eslint/template-parser": "^18.2.12",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/language-service": "^18.2.12",
    "@ngx-formly/schematics": "^6.3.12",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.5.29",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-progress": "^0.0.1",
    "husky": "^9.0.11",
    "jasmine-core": "~5.2.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.2",
    "npm": "^10.5.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test:noWatch"
    }
  },
  "lint-staged": {
    "*.{js,ts,scss,css,html}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add"
    ]
  }
}
